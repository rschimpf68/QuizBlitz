{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"
, "src/app/components/UserSession.jsx", "src/app/components/UserSession.jsx", "src/app/page.jsx/[...nextauth].jsx", "src/app/api/auth/[...nextauth]/route.jsx"  ],
  
  "exclude": [
    "node_modules"
  ],
  // "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"], "api/auth/[...nextauth]/route.jsx]",
  // "exclude": ["node_modules"]
}
